{
  "name": "my-planner2",
  "version": "0.0.0",
  "bundlewatch": {
    "files": [
      {
        "path": "./src/*.js",
        "maxSize": "100kB"
      }
    ]
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve --port 4200 --poll=2000",
    "build": "ng build --stats-json",
    "build-prod-nostat": "node --max_old_space_size=8192 node_modules/@angular/cli/bin/ng build --prod --extra-webpack-config webpack.extra.js",
    "build-prod": "node --max_old_space_size=8192 node_modules/@angular/cli/bin/ng build --prod --stats-json --extra-webpack-config webpack.extra.js --watch",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "bundlewatch": "bundlewatch",
    "compress": "npx gzipper --verbose ./dist/myplanner ./gzipped",
    "stats": "webpack-bundle-analyzer dist/myplanner/stats-es2015.json"
  },
  "private": true,
