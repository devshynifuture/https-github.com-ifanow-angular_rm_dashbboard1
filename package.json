{
  "name": "my-planner2",
  "version": "0.0.0",
  "bundlewatch": {
    "files": [
      {
        "path": "./src/*.js",
        "maxSize": "100kB"
      }
    ]
  },
  "scripts": {
    "ng": "ng",
    "start": "node --max_old_space_size=8192 node_modules/@angular/cli/bin/ng serve --port 4200 --poll=2000",
    "build": "ng build --stats-json",
    "build-prod-nostat": "node --max_old_space_size=8192 node_modules/@angular/cli/bin/ng build --prod --extra-webpack-config webpack.extra.js",
    "build-dev": "node --max_old_space_size=8192 node_modules/@angular/cli/bin/ng build --configuration dev --extra-webpack-config webpack.extra.js",
    "build-staging": "node --max_old_space_size=8192 node_modules/@angular/cli/bin/ng build --configuration staging --extra-webpack-config webpack.extra.js",
    "build-dev-stat": "node --max_old_space_size=8192 node_modules/@angular/cli/bin/ng build --stats-json --extra-webpack-config webpack.extra.js --watch",
    "build-prod": "node --max_old_space_size=8192 node_modules/@angular/cli/bin/ng build --prod --stats-json --extra-webpack-config webpack.extra.js --watch",
    "build-low": "node --max_old_space_size=5192 node_modules/@angular/cli/bin/ng build --configuration dev --extra-webpack-config webpack.extra.js",
    "build-devifanow": "node --max_old_space_size=8192 node_modules/@angular/cli/bin/ng build --configuration dev --extra-webpack-config webpack.extra.js",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "bundlewatch": "bundlewatch",
    "compress": "npx gzipper --verbose ./dist/myplanner ./gzipped",
    "stats": "webpack-bundle-analyzer dist/myplanner/stats.json",
    "hmr": "ng serve --port 4200 --poll=2000 --configuration hmr"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "github:angular/animations-builds#8.2.x",
    "@angular/cdk": "github:angular/cdk-builds#8.2.x",
